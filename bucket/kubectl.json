{
    "homepage": "https://kubernetes.io/docs/user-guide/kubectl-overview/",
    "license": "https://github.com/kubernetes/kubernetes/blob/master/LICENSE",
    "version": "1.5.6",
    "architecture": {
        "64bit": {
            "url": "https://storage.googleapis.com/kubernetes-release/release/v1.5.6/kubernetes-client-windows-amd64.tar.gz",
            "hash": "sha1:8a358899d632f8795a3fbeb9a5d7719e39e53b21"
        },
        "32bit": {
            "url": "https://storage.googleapis.com/kubernetes-release/release/v1.5.6/kubernetes-client-windows-386.tar.gz",
            "hash": "sha1:54f4c34a1d25d965fa61555cf6adf7077a9b3a8f"
        }
    },
    "extract_dir": "kubernetes\\client",
    "bin": [
        "bin\\kubectl.exe",
        "bin\\kubefed.exe"
    ],
    "checkver": {
        "url": "https://storage.googleapis.com/kubernetes-release/release/stable.txt",
        "re": "v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://storage.googleapis.com/kubernetes-release/release/v$version/kubernetes-client-windows-amd64.tar.gz"
            },
            "32bit": {
                "url": "https://storage.googleapis.com/kubernetes-release/release/v$version/kubernetes-client-windows-386.tar.gz"
            }
        },
        "hash": {
            "type": "sha1",
            "mode": "extract",
            "find": "^([a-f0-9]{40})$",
            "url": "$url.sha1"
        }
    }
}
