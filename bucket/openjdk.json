{
    "homepage": "https://github.com/ojdkbuild/ojdkbuild",
    "version": "1.8.0.131-1",
    "license": "GPL2",
    "architecture": {
        "64bit": {
            "url": "https://github.com/ojdkbuild/ojdkbuild/releases/download/1.8.0.131-1/java-1.8.0-openjdk-1.8.0.131-1.b11.ojdkbuild.windows.x86_64.zip",
            "hash": "7e7384636054001499ba96d55c90fc39cbb0441281254a1e9ac8510b527a7a46",
            "extract_dir": "java-1.8.0-openjdk-1.8.0.131-1.b11.ojdkbuild.windows.x86_64"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://raw.githubusercontent.com/ojdkbuild/ojdkbuild/master/README.md",
        "re": "download/.*/java-(?<short>[\\d.]+)-openjdk-(?<version>[\\d.]+-[\\d]+)(?<tail>.*).x86_64.zip\\)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ojdkbuild/ojdkbuild/releases/download/$version/java-$matchShort-openjdk-$version$matchTail.x86_64.zip",
                "extract_dir": "java-$matchShort-openjdk-$version$matchTail.x86_64"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
